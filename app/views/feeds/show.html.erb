<div class="wrapper">
  <h1>詳細ページ</h1>
  <p id="notice"><%= notice %></p>

  <p>
    <strong>コメント:</strong>
    <%= @feed.content %>
  </p>

  <p>
    <strong>Image:</strong>
    <%= image_tag @feed.image.url %>
  </p>

  <p><%= link_to "投稿一覧に戻る", feeds_path %></p>
  <p><%= link_to '編集する', edit_feed_path(@feed) %></p>
</div>
